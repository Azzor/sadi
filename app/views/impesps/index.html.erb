<h1>Catálogo de Impuestos Específicos</h1>

<%= link_to( '+ Agregar Impuesto Específico', new_impesp_path, :class => "btn-action btn-primary") %>
<br />

<table id="tabla-rayada">
  <tr class="titulo">
    <th>Impuesto Específico</th>
    <th>Impuesto Genérico</th>
    <th></th>
    <th></th>
  </tr>

<% @impesps.each do |impesp| %>
  <tr class="lcliente">
    <td><%= impesp.impesp %></td>
    <td><%= impesp.impgen2 %></td>
    <td><%#= link_to 'Show', impesp %></td>
    <td><span class="manage-list-notes do_not_print">(<%= link_to 'Editar', edit_impesp_path(impesp) %> |
      <%= link_to 'baja', impesp, method: :delete, data: { confirm: 'Are you sure?' } %>)</span></td>
  </tr>
<% end %>
</table>

<br> <%= link_to 'Ir al Inicio', :controller => 'access' %>
